
def fizz_buzz_sum(target):
    total = 0
    for num in range(target):
        if num % 3 == 0 or num % 5 == 0:
            total += num
    return total

# Example usage:
target_value = 10
result = fizz_buzz_sum(target_value)
print("Sum of all multiples of 3 or 5 below", target_value, "is:", result)
