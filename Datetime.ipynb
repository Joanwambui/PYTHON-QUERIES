    "import datetime\n",
    "\n",
    "current_year = datetime.datetime.now().year\n",
    "name = input(\"What is your name? \")\n",
    "age = int(input(\"How old are you? \"))\n",
    "print(f\"{name}, you will be 100 years old in {current_year - age + 100}\")\n"
   ]
