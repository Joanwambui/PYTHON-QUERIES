   "source": [
    "import re\n",
    "\n",
    "def is_float_number(s):\n",
    "    pattern = r'^[+-]?(\\d+\\.\\d+|\\.\\d+|\\d+\\.)([eE][+-]?\\d+)?$'\n",
    "    return bool(re.match(pattern, s))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    n = int(input().strip())\n",
    "    for _ in range(n):\n",
    "        s = input().strip()\n",
    "        print(is_float_number(s))\n"
   ]
  }
