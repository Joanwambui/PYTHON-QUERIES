chr_1, chr_2, chr_3 = input().split()

print(chr_1 * 5)
print(chr_1 + chr_2 * 3 + chr_1)
print(chr_1 + chr_2 + chr_3 + chr_2 + chr_1)
print(chr_1 + chr_2 * 3 + chr_1)
print(chr_1 * 5)

