def pos_sum(n):
    total = 0  # Initialize total to 0
    for i in n:  # Iterate through each number in the list
        if i > 0:  # Check if the number is positive
            total += i  # Add positive number to total
    return total  # Return the sum of positive numbers

# Example list
n = [-4, -7, 8, 56, -8]

# Call the function and print the result
print(pos_sum(n))  
