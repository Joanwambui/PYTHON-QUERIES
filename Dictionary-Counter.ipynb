   "source": [
    "from collections import Counter\n",
    "\n",
    "s = input()\n",
    "d = Counter(s)\n",
    "sorted_d = sorted(d.items(), key=lambda x: (-x[1], x[0]))\n",
    "result = ''\n",
    "for char, freq in sorted_d:\n",
    "    result += char * freq\n",
    "print(result)\n",
    "'''\n",
    "Sorting string in descending order eg sum to be msu\n",
    "'''"
   ]
