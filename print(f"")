import sys
import math

# Parse the three dimensions from stdin
length, width, height = map(int, input().split())

# Validate dimensions
if length <= 0 or width <= 0 or height <= 0:
    print("Invalid dimension")
else:
    # Compute volume
    v = length * width * height
    # Print the result
    print(f"The quantity of water in the room is {v} cubic feet.")
