# Python program to print calendar for given year 

# importing calendar library 
import calendar 

def printcalendar(year): 

# printing calendar 
    print(calendar.calendar(year)) 

# driver program to test above function 
year = 2019
printcalendar(year) 


#Deriving month,day 
from datetime import date
x = date.today()
print(x)
month = x.month
print(month)
day = x.day
year = x.year
print(year)


import datetime
tm = datetime.time(2, 25, 50, 13)
print(tm)
02:25:50.000013

import datetime

tm = datetime.time(1, 50, 20, 133257)

print('Time tm is ',
      tm.hour, ' hours ',
      tm.minute, ' minutes ',
      tm.second, ' seconds and ',
      tm.microsecond, ' microseconds')
