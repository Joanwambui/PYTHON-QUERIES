   "source": [
    "def is_anagram(left, right):\n",
    "    left_letters = sorted(left)\n",
    "    right_letters = sorted(right)\n",
    "    return left_letters == right_letters\n",
    "\n",
    "left = \"party\"\n",
    "right = \"dusty\"\n",
    "y = is_anagram(left, right)\n",
    "print(f\"Is '{left}' an anagram of '{right}'? {y}\")\n"
   ]
